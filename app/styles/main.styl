body,
html
  height 100%
  width 100%
  background #399CE3

.title 
  display inline-block
  position absolute
  top 10%
  left 10%
  
h1 
  display inline-block
  margin-right 20px
  color #fff
  
#map 
  position absolute 
  top 20%
  right 10%
  left 10%
  bottom 20%
  
  svg 
    width 100%
    height 100%

#chart 
  width 100%
  position absolute 
  top 80%
  bottom 0

.background
  fill #399CE3

path
  fill white

.route
  stroke orange
  fill orange
  
.highcharts-legend
  display none
  
.highcharts-container 
  height 200px !important
  
  rect 
    fill #399CE3
  
.highcharts-series rect
  width 50
  fill rgba(#fff, .8)
  stroke #399CE3

.highcharts-grid 
  display none

.highcharts-axis-labels 
  display none

  